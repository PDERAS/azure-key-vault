{
    "name": "pderas/azure-key-vault",
    "version": "0.3.1",
    "description": "A package to interact with Azure Key Vault",
    "keywords": [
        "azure",
        "key-vault",
        "vault",
        "secrets",
        "laravel"
    ],
    "homepage": "https://github.com/PDERAS/azure-key-vault",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pderas\\AzureKeyVault\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\":"tests/"
        }
    },
    "authors": [
        {
            "name": "Brock Roadhouse",
            "email": "brock.roadhouse@fabledsolutions.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/filesystem": "^11.22|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pderas\\AzureKeyVault\\AzureKeyVaultServiceProvider"
            ],
            "aliases": {
                "AzureKeyVault": "Pderas\\AzureKeyVault\\Facades\\AzureKeyVaultFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
